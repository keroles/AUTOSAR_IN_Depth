
/*
 * MSP430 Specific configuration file for Launchpad SDK
 */

IMPLEMENTATION lauchpad {
  OS {
    BOOLEAN [
      TRUE {
        ENUM [
          serial {
            UINT32 RXBUFFER = 32;
            UINT32 TXBUFFER = 16;
          }
        ] LIBRARY[];
      },
      FALSE
    ] BUILD = FALSE;
  };
};

CPU launchpad {
  LIBRARY serial {
    PATH = "msp430x/small/msp430fr5994/launchpad/serial";
    CHEADER = "serial.h";
    CFILE   = "serial.c";
  };
};
