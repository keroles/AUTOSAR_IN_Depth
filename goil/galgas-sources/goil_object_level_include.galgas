grammar goil_object_level_include "LL1" {
  syntax goil_syntax
  <oil_declaration_list>
    ?let @implementationObjectMap types
    ?!@objectAttributes identifiers

  unused <OIL_version>
  unused <application_definition>
  unused <file>
  unused <implementation_definition>
  unused <include_file_level>
  unused <start>
  unused <object_definition_list>
  unused <include_cpu_level>
  unused <file_without_include>
}

